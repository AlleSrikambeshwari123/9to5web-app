<!DOCTYPE html>
<html lang="en">
  <head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <title>Login</title>
    <meta
      content="width=device-width, initial-scale=1.0, shrink-to-fit=no"
      name="viewport"
    />
    <link
      rel="icon"
      href="/img/favicon.png"
      type="image/x-icon"
    />
    <link rel="icon" href="/img/favicon.png" type="image/x-icon" />


    <!-- Fonts and icons -->
    <link
      href="http://fonts.googleapis.com/css?family=Lato:300,400,700,900"
      media="all"
    />
    <link rel="stylesheet" href="/assets/css/fonts.min.css" media="all" />
    <link rel="stylesheet" href="/assets/css/fonts.min.css" media="all" />


    <!-- CSS Files -->
    <link rel="stylesheet" href="/assets/css/bootstrap.min.css" />
    <link rel="stylesheet" href="/assets/css/atlantis.min.css" />
    <link rel="stylesheet" href="/assets/css/jquery.toast.min.css" />
    <% try{ if(isPostbox) { %> 

    <link rel="stylesheet" href="/assets/css/bootstrap.min.css" />
    <link rel="stylesheet" href="/assets/css/atlantis.min.css" />
    <link rel="stylesheet" href="/assets/css/jquery.toast.min.css" />
    <% }}catch(e){} %> 


  </head>

  <body class="login">
    <div class="wrapper wrapper-login wrapper-login-full p-0">
      <div
        class="
          login-aside
          w-50
          d-flex
          flex-column
          align-items-center
          justify-content-center
          text-center
          bg-primary-gradient
        "
      >
        <h1 class="title fw-bold text-white mb-3"><%= process.CLIENT_URL == "postboxesetc" ? "Post Boxes ETC" : "9-5 Import Portal" %></h1>
        <p class="subtitle text-white op-7"><%= process.CLIENT_URL == "postboxesetc" ? "Much More Than A Post Box" : "Always On Time" %></p>
      </div>
      <div
        class="
          login-aside
          w-50
          d-flex
          align-items-center
          justify-content-center
          bg-white
        "
      >
        <div
          class="
            container container-login container-transparent
            animated
            fadeIn
          "
        >
        <% if(isPostbox) { %> 
        <center>  <img src="/img/postboxlogo.jpg" class="m-3"  width=300"/><br /></center>
          <% } else { %>
          <img src="/img/logo.jpg" class="m-3" /><br />
          <% } %>

          <h3 class="text-center">Reset Customer Password</h3>
          <div class="login-form">
            <div class="form-group">
              <label for="username" class="placeholder"><b>Email</b></label>
              <input
                id="reset_password_username"
                name="username"
                type="text"
                class="form-control"
                required
              />
            </div>
            <div class="form-group wrongPassword" style="display: none">
              <span class="msg" style="color: #6c0f0f"
                >Wrong Email Address.</span
              >
            </div>
            <div class="form-group form-action-d-flex mb-3">
              <% if(isPostbox) {%>
              <a href="/customer/postbox/login" id="back" class="link">Back</a>
              <% }else{%>
                <a href="/customer/login" id="back" class="link">Back</a>

              <% }%>

              <a
                href="#"
                id="customer_send_request"
                class="
                  btn btn-primary
                  col-md-5
                  float-right
                  mt-3 mt-sm-0
                  fw-bold
                "
                style="background-color: #6c0f0f"
                >Send Request</a
              >
            </div>
          </div>
        </div>
        <div
          class="
            container container-signup container-transparent
            animated
            fadeIn
          "
          style="display: none"
        >
        <% if(isPostbox) { %> 

          <center>  <img src="/img/postboxlogo.jpg" class="m-3"  width="300" /><br /></center>

          <% }else{ %>
          <img src="/img/logo.jpg" class="m-4" />
          <% } %>
          <br />
          <h3 class="text-center">Email Sent</h3>
          <div class="login-form">
            <span class="msg"
              >A request has been sent to your email address. Please check your
              email.</span
            >
            <div class="form-group form-action-d-flex mb-3">
              <%if(isPostbox){ %>
              <a
                href="/customer/postbox/login"
                id="back2"
                class="
                  btn btn-primary
                  col-md-5
                  float-right
                  mt-3 mt-sm-0
                  fw-bold
                "
                style="background-color: #6c0f0f"
                >Back</a
              >
              <% }else{ %>
                <a
                href="/customer/login"
                id="back2"
                class="
                  btn btn-primary
                  col-md-5
                  float-right
                  mt-3 mt-sm-0
                  fw-bold
                "
                style="background-color: #6c0f0f"
                >Back</a
              >
              <% } %>
            </div>
          </div>
        </div>
      </div>
    </div>
    <!--<script>window.$ = window.jQuery = require('../assets/js/core/jquery.3.2.1.min.js');</script>
	-->
    <script src="/assets/js/core/jquery.3.2.1.min.js"></script>
    <script src="/assets/js/plugin/jquery-ui-1.12.1.custom/jquery-ui.min.js"></script>
    <script src="/assets/js/core/popper.min.js"></script>
    <script src="/assets/js/core/bootstrap.min.js"></script>
    <script src="/js/jquery.toast.min.js"></script>
    <script src="/js/customer/forgot_password.js"></script>
    <% try{ if(isPostbox) { %> 

    <script src="/assets/js/core/jquery.3.2.1.min.js"></script>
    <script src="/assets/js/plugin/jquery-ui-1.12.1.custom/jquery-ui.min.js"></script>
    <script src="/assets/js/core/popper.min.js"></script>
    <script src="/assets/js/core/bootstrap.min.js"></script>
    <script src="/js/jquery.toast.min.js"></script>
    <script src="/js/customer/forgot_password.js"></script>
    <% }}catch(e){} %> 


  </body>
</html>
